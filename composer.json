{
    "name": "dkplus/indicator",
    "description": "Issue tracking within your application with a custom backend.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Pfeifer-Bley",
            "email": "oskar@programming-php.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dkplus\\Indicator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Dkplus\\Indicator\\": "spec/",
            "feature\\Dkplus\\Indicator\\": "features/bootstrap/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "prooph/event-sourcing": "^4.0",
        "mattketmo/uuid-2x-bridge": "*@dev",
        "ramsey/uuid": "^3.5",
        "prooph/event-store": "^6.3",
        "m4tthumphrey/php-gitlab-api": "^7.13",
        "beberlei/assert": "^2.6",
        "dkplus/formica": "^0.1.0"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "phpspec/phpspec": "@dev",
        "ciaranmcnulty/phpspec-typehintedmethods": "^2.0",
        "bossa/phpspec2-expect": "^2.0",
        "doctrine/dbal": "^2.5",
        "nayjest/builder": "^2.1",
        "fzaninotto/faker": "^1.6",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "config": {
        "bin-dir": "bin"
    }
}
